{
  "name": "accounts",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "nodemon ./bin/www",
    "compile:dev": "tsc -w",
    "generate:migration": "knex migrate:make migration_name --knexfile=./dist/database/knexfile.js",
    "migrate:dev": "knex migrate:latest --knexfile=./dist/database/knexfile.js",
    "migrate:down": "knex migrate:down --knexfile=./dist/database/knexfile.js",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "compile": "tsc",
    "heroku-postbuild": "npm run compile && npm run migrate"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "async": "^3.2.3",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jest": "^27.5.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.3",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "npm": "^8.5.3",
    "supertest": "^6.2.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@types/async": "^3.2.12",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^27.4.1",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  }
}
